{
    "name": "RuffleBlocker",
    "version": "0",
    "manifest_version": 3,
    "minimum_chrome_version": "113.0",
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["inject.js"],
        "run_at": "document_start",
        "world": "MAIN"
      }
    ],
    "web_accessible_resources": [{
      "resources": ["injected.js"],
      "matches": ["<all_urls>"]
    }],
    "content_security_policy": {
      "extension_pages": "default-src 'self' 'wasm-unsafe-eval';"
    }
  }